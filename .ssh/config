Host *
  ServerAliveInterval 240
  PreferredAuthentications publickey
  ForwardAgent yes


Host 10.0.*.*
  User ubuntu
  StrictHostKeyChecking no
  ProxyCommand ssh -W %h:%p ubuntu@b-eu1.doofinder.com

Host 10.1.*.*
  User ubuntu
  StrictHostKeyChecking no
  ProxyCommand ssh -W %h:%p ubuntu@b-us1.doofinder.com

Host bastion
  HostName b-eu1.doofinder.com
  User ubuntu
  ForwardAgent yes

Host usbastion
  HostName b-us1.doofinder.com
  User ubuntu
  ForwardAgent yes

Host tunnel_stats
  Hostname b-eu1.doofinder.com
  LocalForward 0.0.0.0:10002 stats.clickhouse.internal.doofinder.com:8123
  User ubuntu

Host tunnel_stats_us
  Hostname b-us1.doofinder.com
  LocalForward 0.0.0.0:10002 stats.clickhouse.internal.doofinder.com:8123
  User ubuntu

Host tunnel_searches
    HostName b-eu1.doofinder.com
    LocalForward 0.0.0.0:10001 searches.clickhouse.internal.doofinder.com:8123
    User ubuntu

Host layer-pg
  HostName b-eu1.doofinder.com
  LocalForward 0.0.0.0:10101 postgres-layer.internal.doofinder.com:5432
  User ubuntu

Host layer-pg_us
  HostName b-us1.doofinder.com
  LocalForward 0.0.0.0:10102 postgres-layer.internal.doofinder.com:5432
  User ubuntu

Host tunnel_mysql_us
  HostName b-us1.doofinder.com
  LocalForward 0.0.0.0:33066 mysql.internal.doofinder.com:3306
  User ubuntu

Host tunnel_mysql
  HostName b-eu1.doofinder.com
  LocalForward 0.0.0.0:33065 mysql.internal.doofinder.com:3306
  User ubuntu

Host tunnel_mysql_global
